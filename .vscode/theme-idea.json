    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.character"
            ],
            "settings": {
                "foreground": "#008000"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.modifier",
                "storage.type"
            ],
            "settings": {
                "foreground": "#0000ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#795e26"
            }
        },
        {
            "name": "Classes, Types",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#267f99",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables & Parameters",
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other"
            ],
            "settings": {
                "foreground": "#001080"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "variable.language",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#0070c1"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#098658"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Imports & Namespaces",
            "scope": [
                "keyword.control.import",
                "entity.name.namespace"
            ],
            "settings": {
                "foreground": "#267f99"
            }
        },
        {
            "name": "Decorators & Annotations",
            "scope": [
                "meta.decorator",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "#cc00a3"
            }
        },
        {
            "name": "Tags (HTML/XML)",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#800000"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#e50000"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name.css"
            ],
            "settings": {
                "foreground": "#0451a5"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#000080",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#0000ff"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#0451a5"
            }
        }
    ]
}